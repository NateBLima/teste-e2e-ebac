pipeline{
    agent any

    stages{
        stage('clonar repositorio'){
            steps{
               git branch: 'main', url: 'https://github.com/NateBLima/teste-e2e-ebac.git'
            }
        }
        stage('instalar dependencias'){
            steps{
                sh 'npm install'
            }
        }
        stage('executar testes'){
            steps{
                echo 'implementando'
 //              sh 'npx cypress run'
            }
        }
    }
}