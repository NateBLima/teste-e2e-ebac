pipeline{
    agent any

    stages{
        stage('clonar repositorio'){
            steps{
               bat 'git branch: \'main\', url: \'https://github.com/NateBLima/teste-e2e-ebac.git\''
            }
        }
        stage('instalar dependencias'){
            steps{
                bat 'npm install'
            }
        }
        stage('executar testes'){
            steps{
                echo 'implementando'
 //              sh 'npx cypress run'
            }
        }
    }
}