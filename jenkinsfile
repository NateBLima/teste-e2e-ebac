pipeline {
    agent any

    stages {
        stage('clonar repositorio'){
            steps {
                 sh 'git clone https://github.com/NateBLima/teste-e2e-ebac.git'
            }
        }
        stage('instalar dependencias'){
            steps{
                bat 'npm install'
            }
        }
        stage('executar testes'){
            steps{
               sh 'npx cypress run'
            }
        }
    }
}